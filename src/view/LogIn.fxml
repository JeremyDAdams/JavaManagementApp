<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LogInController">
   <children>
      <Button fx:id="submitButton" layoutX="407.0" layoutY="248.0" mnemonicParsing="false" onAction="#submitBtnClick" text="Submit" />
      <TextField fx:id="userField" layoutX="227.0" layoutY="152.0" />
      <TextField fx:id="passField" layoutX="227.0" layoutY="248.0" />
      <Label fx:id="userLabel" layoutX="227.0" layoutY="128.0" text="User ID:" />
      <Label fx:id="passwordLabel" layoutX="227.0" layoutY="220.0" text="Password:" />
      <Label fx:id="headingLabel" layoutX="227.0" layoutY="73.0" style="-fx-font-weight: bold;" text="Please log in to continue" />
      <Label fx:id="locationLabel" layoutX="30.0" layoutY="333.0" text="Label" />
      <Button fx:id="exitButton" layoutX="519.0" layoutY="350.0" mnemonicParsing="false" onAction="#exitBtnClick" text="Exit" />
      <Label fx:id="timeZoneLabel" layoutX="30.0" layoutY="363.0" text="Label" />
   </children>
</AnchorPane>
